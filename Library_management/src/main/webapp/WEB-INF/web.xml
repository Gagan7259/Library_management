<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="https://jakarta.ee/xml/ns/jakartaee"
	xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd"
	id="WebApp_ID" version="5.0">
	<!-- REGISTRATION PAGE MAPPING -->
	<servlet>
		<servlet-name>RegistrationServle</servlet-name>
		<servlet-class>com.servletss.RegistrationServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RegistrationServle</servlet-name>
		<url-pattern>/RegistrationServlet</url-pattern>
	</servlet-mapping>


	<!-- LOGIN PAGE MAPPING -->

	<servlet>
		<servlet-name>LoginservleTt</servlet-name>
		<servlet-class>com.servletss.LoginservleT</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginservleTt</servlet-name>
		<url-pattern>/LoginservleT</url-pattern>
	</servlet-mapping>

	<!-- ADD BOOKS SERVLET -->
	<servlet>
		<servlet-name>AddBookServlett</servlet-name>
		<servlet-class>com.servletss.AddBookServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddBookServlett</servlet-name>
		<url-pattern>/AddBookServlet</url-pattern>
	</servlet-mapping>


	<!-- Assign Books Servlet -->

	<servlet>
		<servlet-name>AssignBooksSevlet</servlet-name>
		<servlet-class>com.servletss.AssignBooksSevlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>AssignBooksSevlet</servlet-name>
		<url-pattern>/AssignBooksSevlet</url-pattern>
	</servlet-mapping>

</web-app>